package listDemo;

public class ArrayDemo 
{
	public int[] data=new int[]{34,53,87,3,98};
	
	public void readArray()
	{
		
		
		
		//if datatype is not constant forever use this .Var takes all datatypes
				//------------------------------------
				for(var d:data)
					System.out.println(d);	
				//-----------------------------------------
				
		
		
		
	}
	
	
	
	
	
	
	

}
